{
  "name": "cut",
  "version": "1.2.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run build-themes && ng serve",
		"build": "npm-run-all build-lib build-app",
		"build-watch": "ng build --prod cut-lib --watch",
    "build-app": "ng build --prod cut-app",
    "build-lib": "ng build --prod cut-lib",
    "build-droid": "tns run android --bundle ns-cut-lib",
    "build-lib-watch": "npm run build-lib && ng build cut-lib --watch",
    "build-schematics": "tsc -p projects/cut-lib/schematics/tsconfig.json",
    "build-themes": "bash ./tools/build-themes.sh",
    "bundle-lib-scss": "npm-run-all --parallel bundle-lib-cut-scss bundle-lib-vars-scss",
    "bundle-lib-cut-scss": "scss-bundle -e projects/cut-lib/src/_cut.scss -d dist/cut/cut.scss",
    "bundle-lib-vars-scss": "scss-bundle -e projects/cut-lib/src/stylesheets/_cut-core.scss -d dist/cut/stylesheets/cut-core.scss",
    "compile-lib-css": "npm-run-all --parallel compile-lib-cut-css",
    "compile-lib-cut-css": "node-sass projects/cut-lib/src/_cut.scss dist/cut/cut.css",
    "copy-lib-assets": "copyfiles -u 4 \"projects/cut-lib/src/assets/**/*\" dist/cut/assets",
    "copy-lib-scss": "copyfiles -f projects/cut-lib/src/stylesheets/utilities/cut-utilities.scss dist/cut/stylesheets/utilities",
    "copy-schematics-files": "copyfiles -u 4 \"projects/cut-lib/schematics/src/**/*.json\" dist/cut/schematics",
    "deploy-docs": "ng build --prod cut-app --base-href https://cbdr.github.io/cb-ux-toolkit/ --output-path docs",
    "test": "ng test",
    "test-ci": "npm-run-all test-lib-ci test-app-ci",
    "test-lib": "ng test cut-lib --source-map",
    "test-lib-ci": "ng test cut-lib --browsers=ChromeHeadless --watch=false --code-coverage",
    "test-app-ci": "ng test cut-app --browsers=ChromeHeadless --watch=false --code-coverage",
    "lint": "ng lint",
    "lint-fix": "ng lint -c fix",
    "e2e": "ng e2e",
    "postbuild-lib": "npm-run-all --parallel copy-lib-assets copy-lib-scss bundle-lib-scss compile-lib-css build-schematics",
    "postbuild-schematics": "npm-run-all copy-schematics-files",
    "posttest-app-ci": "mv coverage coverage-app",
    "posttest-lib-ci": "mv coverage coverage-lib",
    "pretest-app-ci": "rimraf coverage coverage-app",
    "pretest-lib-ci": "rimraf coverage coverage-lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cbdr/cut.git"
  },
  "author": "Michael Gardner <Michael.Gardner@careerbuilderscreening.com>",
  "license": "ISC",
  "homepage": "https://github.com/cbdr/cut#readme",
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.7",
    "@angular/cdk": "~7.3.3",
    "@angular/common": "~7.2.7",
    "@angular/compiler": "^7.2.7",
    "@angular/core": "~7.2.7",
    "@angular/forms": "~7.2.7",
    "@angular/http": "~7.2.7",
    "@angular/material": "~7.3.3",
    "@angular/material-examples": "https://github.com/angular/material2-docs-content.git",
    "@angular/material-moment-adapter": "^7.2.1",
    "@angular/platform-browser": "~7.2.7",
    "@angular/platform-browser-dynamic": "~7.2.7",
    "@angular/router": "~7.2.7",
    "@nativescript/schematics": "^0.5.2",
    "@ngx-prism/core": "^2.0.1",
    "@ngx-prism/rxjs": "^0.1.5",
    "@types/clipboard": "^2.0.1",
    "clipboard": "^2.0.1",
    "core-js": "^2.5.4",
    "font-awesome": "~4.7.0",
    "hammerjs": "^2.0.8",
    "jquery": "~3.3.0",
    "moment": "^2.24.0",
    "nativescript-angular": "^7.2.3",
    "nativescript-theme-core": "^1.0.4",
    "popper.js": "~1.14.0",
    "rxjs": "^6.4.0",
    "zone.js": "~0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.4",
    "@angular-devkit/build-ng-packagr": "~0.13.4",
    "@angular-devkit/core": "^7.3.4",
    "@angular-devkit/schematics": "^7.3.4",
    "@angular/cli": "~7.3.4",
    "@angular/compiler-cli": "^7.2.7",
    "@angular/language-service": "^7.2.7",
    "@schematics/angular": "^7.3.4",
    "@types/hammerjs": "^2.0.36",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "@types/parse5": "^5.0.0",
    "codelyzer": "^4.5.0",
    "copyfiles": "^2.1.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "nativescript-dev-webpack": "^0.21.0",
    "ng-packagr": "^4.7.1",
    "node-sass": "^4.9.3",
    "npm-run-all": "^4.1.3",
    "protractor": "~5.4.0",
    "rimraf": "^2.6.2",
    "scss-bundle": "^2.4.0",
    "ts-node": "~5.0.1",
    "tsickle": "^0.34.3",
    "tslib": "^1.9.0",
    "tslint": "~5.9.1",
    "typescript": "~3.2.4"
  },
  "nativescript": {
    "id": "com.nscut.careerbuilder",
    "tns-ios": {
      "version": "5.1.1"
    },
    "tns-android": {
      "version": "5.2.1"
    }
  }
}
